{
  "name": "coverage-experiment",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "rm -rf .nyc_output && mkdir .nyc_output && jest",
    "report": "nyc report",
    "cover": "npm test; npm run report",
    "start": "node dist/api/api.js",
    "build-api": "rm -rf dist/api && npx parcel build src/api/api.js -d dist/api --target node",
    "build-ui": "rm -rf dist/ui && npx parcel build src/ui/ui.html -d dist/ui --public-url ./",
    "build": "rm -rf dist && npm run build-api && npm run build-ui"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "babel-plugin-istanbul": "6.0.0",
    "jest": "26.6.3",
    "node-fetch": "2.6.1",
    "nyc": "15.1.0",
    "parcel": "1.12.4",
    "pm2": "4.5.1",
    "taiko": "1.1.2",
    "uuid": "8.3.2"
  },
  "jest": {
    "testEnvironment": "node",
    "bail": 1
  },
  "babel": {
    "plugins": [
      "istanbul"
    ]
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}